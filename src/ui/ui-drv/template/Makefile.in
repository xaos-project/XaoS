CC     = @CC@
CFLAGS = @CFLAGS@
LIBS   = @LIBS@ -lm
LFLAGS = @LDFLAGS@
#AR     = @AR@
RANLIB = @RANLIB@

SRCS = ui_template.c 

OBJS = $(SRCS:.c=.o)

TLIB = @LIBPATH@/libuitemplate.a


all:	$(TLIB)

$(TLIB):$(OBJS)
	rm -f $@
	$(AR) rc $@ $^
	$(RANLIB) $@ 

clean:
	rm -f $(TLIB)
	rm -f *.[oas]
	rm -f *~
	rm -f core 

distclean:clean
	rm  Makefile

